<pretty-config xmlns="http://ocpsoft.org/schema/rewrite-config-prettyfaces" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation="http://ocpsoft.org/schema/rewrite-config-prettyfaces
                      http://ocpsoft.org/xml/ns/prettyfaces/rewrite-config-prettyfaces.xsd">

  <url-mapping id="root">
		<pattern value="/" />
		<view-id value="/index.xhtml" />
	</url-mapping>
  
  <url-mapping id="login">
		<pattern value="/login" />
		<view-id value="/auth/login.xhtml" />
	</url-mapping>
  
  <url-mapping id="register">
		<pattern value="/register" />
		<view-id value="/auth/customer-register.xhtml" />
	</url-mapping>
  
	<url-mapping id="management-register">
		<pattern value="/management/register" />
		<view-id value="/auth/register.xhtml" />
	</url-mapping>
  
  <url-mapping id="management-dashboard">
		<pattern value="/management/dashboard" />
		<view-id value="/management/dashboard.xhtml" />
	</url-mapping>
  
  <url-mapping id="management-customers">
		<pattern value="/management/customers" />
		<view-id value="/management/customer.xhtml" />
	</url-mapping>
  
  <url-mapping id="management-food">
		<pattern value="/management/food" />
		<view-id value="/management/food.xhtml" />
	</url-mapping>
  
  <url-mapping id="management-feedback">
		<pattern value="/management/feedback" />
		<view-id value="/management/feedback.xhtml" />
	</url-mapping>
  
  <url-mapping id="management-staff">
		<pattern value="/management/management-staff" />
		<view-id value="/management/management-staff.xhtml" />
	</url-mapping>  

  <url-mapping id="management-delivery">
		<pattern value="/management/delivery-staff" />
		<view-id value="/management/delivery-staff.xhtml" />
	</url-mapping>
  
  <url-mapping id="management-reservation">
		<pattern value="/management/reservation-staff" />
		<view-id value="/management/reservation-staff.xhtml" />
	</url-mapping>
  
  <url-mapping id="management-profile">
		<pattern value="/management/profile" />
		<view-id value="/management/profile.xhtml" />
	</url-mapping>
  
  



  <url-mapping id="reservation-dashboard">
		<pattern value="/reservation/dashboard" />
		<view-id value="/reservation/dashboard.xhtml" />
	</url-mapping>
  
  <url-mapping id="reservation-orders">
		<pattern value="/reservation/orders" />
		<view-id value="/reservation/order.xhtml" />
	</url-mapping>
  
   <url-mapping id="reservation-orders-view">
		<pattern value="/reservation/orders/#{id}" />
		<view-id value="/reservation/order-show.xhtml" />
	</url-mapping>
  
  
  <url-mapping id="reservation-profile">
		<pattern value="/reservation/profile" />
		<view-id value="/reservation/profile.xhtml" />
	</url-mapping>


  <url-mapping id="delivery-dashboard">
		<pattern value="/delivery/dashboard" />
		<view-id value="/delivery/dashboard.xhtml" />
	</url-mapping>
  
  <url-mapping id="delivery-orders">
		<pattern value="/delivery/orders" />
		<view-id value="/delivery/order.xhtml" />
	</url-mapping>
  
   <url-mapping id="delivery-orders-view">
		<pattern value="/delivery/orders/#{id}" />
		<view-id value="/delivery/order-show.xhtml" />
	</url-mapping>
  
  
  <url-mapping id="delivery-profile">
		<pattern value="/delivery/profile" />
		<view-id value="/delivery/profile.xhtml" />
	</url-mapping>



  
  
  <url-mapping id="menu">
		<pattern value="/menu" />
		<view-id value="/customer/menu.xhtml" />
	</url-mapping>
  <url-mapping id="e-wallet">
		<pattern value="/e-wallet" />
		<view-id value="/customer/e-wallet.xhtml" />
	</url-mapping>
  <url-mapping id="feedback">
		<pattern value="/feedback" />
		<view-id value="/customer/feedback.xhtml" />
	</url-mapping>
  
  <url-mapping id="order">
		<pattern value="/orders" />
		<view-id value="/customer/orders.xhtml" />
	</url-mapping>
  
  <url-mapping id="customer-order-details">
		<pattern value="/orders/#{id}" />
		<view-id value="/customer/order-show.xhtml" />
  </url-mapping>

  <url-mapping id="profile">
		<pattern value="/profile" />
		<view-id value="/customer/profile.xhtml" />
	</url-mapping>
  
  
  
 
</pretty-config>
