<pretty-config xmlns="http://ocpsoft.org/schema/rewrite-config-prettyfaces" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation="http://ocpsoft.org/schema/rewrite-config-prettyfaces
                      http://ocpsoft.org/xml/ns/prettyfaces/rewrite-config-prettyfaces.xsd">

  <url-mapping id="root">
		<pattern value="/" />
		<view-id value="/index.xhtml" />
	</url-mapping>
  
  <url-mapping id="login">
		<pattern value="/login" />
		<view-id value="/auth/login.xhtml" />
	</url-mapping>
  
  <url-mapping id="register">
		<pattern value="/register" />
		<view-id value="/auth/customer-register.xhtml" />
	</url-mapping>
  
	<url-mapping id="management-register">
		<pattern value="/management/register" />
		<view-id value="/auth/register.xhtml" />
	</url-mapping>
  
  <url-mapping id="management-dashboard">
		<pattern value="/management/dashboard" />
		<view-id value="/management/dashboard.xhtml" />
	</url-mapping>
  
  <url-mapping id="management-customers">
		<pattern value="/management/customers" />
		<view-id value="/management/customer.xhtml" />
	</url-mapping>
  
  <url-mapping id="management-food">
		<pattern value="/management/food" />
		<view-id value="/management/food.xhtml" />
	</url-mapping>
  
  <url-mapping id="management-feedback">
		<pattern value="/management/feedback" />
		<view-id value="/management/feedback.xhtml" />
	</url-mapping>
  
  <url-mapping id="management-staff">
		<pattern value="/management/management-staff" />
		<view-id value="/management/management-staff.xhtml" />
	</url-mapping>  

  <url-mapping id="management-delivery">
		<pattern value="/management/delivery-staff" />
		<view-id value="/management/delivery-staff.xhtml" />
	</url-mapping>
  
  <url-mapping id="management-reservation">
		<pattern value="/management/reservation-staff" />
		<view-id value="/management/reservation-staff.xhtml" />
	</url-mapping>
  
  <url-mapping id="customer-dashboard">
		<pattern value="/customer/dashboard" />
		<view-id value="/customer/dashboard.xhtml" />
	</url-mapping>
 
</pretty-config>
